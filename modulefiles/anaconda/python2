#%Module
#
proc ModulesHelp { } {
        puts stderr " "
        puts stderr "\t This module installs anaconda"
        puts stderr " "
}

conflict anaconda/python3

module-whatis   "installs anaconda"

set package_root /home/srinivm/Ubuntu-14.04/software/anaconda2
                 

prepend-path PATH ${package_root}/bin
